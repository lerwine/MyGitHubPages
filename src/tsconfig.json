{
  "compileOnSave": true,
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "es6",
    "module": "commonjs",
    "lib": [
      "es2017",
      "dom"
    ]
  },
  "include": [ "App/**/*" ],
  "exclude": [
    "packages",
    "wwwroot",
    "lib",
    "obj",
    "bin",
    "content",
    "Scripts"
  ]
}
